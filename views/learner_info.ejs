<!doctype html>
<div class="bradcam_area breadcam_bg overlay2">
  <h3>학습자 정보</h3>
</div>

<div class="popular_courses">
  <div class="all_courses">
    <div class="container">
      <table class="table text-center">
        <thead>
          <tr>
            <th>순번</th>
            <th>아이디</th>
            <th>이름</th>
            <th>진도</th>
            <th>퀴즈</th>
          </tr>
          <thead>
          <tbody>
            <%  data.forEach(function (item, index) { %>
            <tr>
              <td><%= index+1 %></td>
              <td><%= item.user_id %></td>
              <td><%= item.name %></td>
              <%
                var hour = parseInt(item.progress/3600);
                var min = parseInt((item.progress%3600)/60);
                var sec = item.progress%60;
                if (hour.toString().length==1) hour = "0" + hour;
	              if (min.toString().length==1) min = "0" + min;
                if (sec.toString().length==1) sec = "0" + sec;
              %>
              <td><%=hour %>:<%=min%>:<%=sec%></td>
              <td><%= quiz[index].sum %>/<%= cnt[0].cnt %> </td>
            </tr>
            <% }); %>
          </tbody>
      </table>
    </div>
  </div>
</div>
